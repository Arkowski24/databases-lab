<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="pl.edu.agh.hibernate.example.shopapp.presenter.EditItemPresenter">
    <children>
        <TextField fx:id="productNameTextField" editable="false" layoutX="28.0" layoutY="54.0"
                   AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="30.0"/>
        <Label layoutX="14.0" layoutY="31.0" text="Product Name" AnchorPane.leftAnchor="10.0"
               AnchorPane.topAnchor="10.0"/>
        <Label layoutX="14.0" layoutY="88.0" text="Ordered Units" AnchorPane.leftAnchor="10.0"
               AnchorPane.rightAnchor="113.0" AnchorPane.topAnchor="110.0"/>
        <TextField fx:id="unitsInStockTextField" editable="false" layoutX="28.0" layoutY="111.0"
                   AnchorPane.leftAnchor="10.0"
                   AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="80.0"/>
        <Button fx:id="saveButton" layoutX="126.0" layoutY="58.0" mnemonicParsing="false" onAction="#handleOkEvent"
                prefWidth="60.0"
                text="Save" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="80.0"/>
        <Button layoutX="70.0" layoutY="125.0" mnemonicParsing="false" onAction="#handleCancelEvent" prefWidth="60.0"
                text="Cancel" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0"/>
        <TextField fx:id="orderedUnitsTextField" layoutX="38.0" layoutY="121.0"
                   AnchorPane.leftAnchor="10.0"
                   AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="130.0"/>
        <Label layoutX="24.0" layoutY="98.0" text="Units In Stock" AnchorPane.leftAnchor="10.0"
               AnchorPane.topAnchor="60.0"/>
    </children>
</AnchorPane>
